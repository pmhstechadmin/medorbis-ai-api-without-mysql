{
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/v1/(.*)", "dest": "/api/index.py" },
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/(health|docs|openapi.json|v2/.*)", "dest": "/api/index.py" }
  ]
}
